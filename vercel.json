{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/auth/1",
      "dest": "/api/auth/1.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/auth/2",
      "dest": "/api/auth/2.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/auth/3",
      "dest": "/api/auth/3.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
